{
	"name": "tasgiving-2020-bundle",
	"version": "1.0.0",
	"description": "A tutorial package using p5.js",
	"author": {
		"name": "Ewan lyon"
	},
	"files": [
		"dashboard",
		"graphics",
		"extension"
	],
	"nodecg": {
		"compatibleRange": "1.7.2",
		"dashboardPanels": [
			{
				"name": "runs-schedule",
				"title": "Schedule",
				"width": 3,
				"file": "runs/schedule.html",
				"headerColor": "#525F78",
				"workspace": "Runs"
			},
			{
				"name": "runs-current",
				"title": "Current",
				"width": 3,
				"file": "runs/current.html",
				"headerColor": "#525F78",
				"workspace": "Runs"
			},
			{
				"name": "runs-next",
				"title": "Next",
				"width": 3,
				"file": "runs/next.html",
				"headerColor": "#525F78",
				"workspace": "Runs"
			},
			{
				"name": "timeClock",
				"title": "Time Clock",
				"width": 3,
				"file": "timeClock/timeClock.html",
				"headerColor": "#525F78",
				"workspace": "Runs"
			},
			{
				"name": "omnibar",
				"title": "Omnibar",
				"width": 3,
				"file": "omnibar/omnibar.html",
				"headerColor": "#525F78",
				"workspace": "Event"
			}
		],
		"graphics": [
			{
				"file": "omnibar/omnibar.html",
				"width": 1920,
				"height": 70
			},
			{
				"file": "upNext/upNext.html",
				"width": 900,
				"height": 835
			},
			{
				"file": "runs/1p4x3.html",
				"width": 560,
				"height": 385
			},
			{
				"file": "runs/1p16x9.html",
				"width": 1445,
				"height": 180
			},
			{
				"file": "runs/2p4x3.html",
				"width": 566,
				"height": 960
			},
			{
				"file": "runs/2p16x9.html",
				"width": 1884,
				"height": 386
			}
		]
	},
	"scripts": {
		"start": "node ../.. --bundlesEnabled=tasgiving-2020-bundle"
	},
	"dependencies": {
		"cheerio": "^1.0.0-rc.3",
		"clone": "^2.1.2",
		"deep-diff": "^1.0.2",
		"deep-equal": "^2.0.4",
		"lodash": "^4.17.20",
		"lodash.assign": "^4.2.0",
		"lodash.merge": "^4.6.2",
		"normalize.css": "^8.0.1",
		"object-path": "^0.11.5",
		"request-promise-native": "^1.0.9"
	}
}
